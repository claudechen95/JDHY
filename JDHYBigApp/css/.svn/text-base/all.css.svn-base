body {
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	font-size: 20px;
	font-family: Microsoft YaHei;
}
ul,
ol {
	list-style: none;
	margin: 0;
	padding: 0;
}
a,
a:hover,
a:active,
a:visited {
	text-decoration: none;
	color: #000000;
}
.clear {
	clear: both;
}
/*去掉video的全屏按钮*/

video::-webkit-media-controls-fullscreen-button {
	display: none;
}
video {
	background-color: #000000;
}
/*防止溢出*/

.mui-content {
	position: relative;
	overflow: hidden;
}
/*大背景*/

#img-bg {
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 1;
}